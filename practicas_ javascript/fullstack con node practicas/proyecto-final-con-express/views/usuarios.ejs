<!DOCTYPE html>
<html lang="en">
<head>
  <% include partials/head%>
    <title>GonzaloStore</title>
</head>
<header>
  <% include partials/header%>
</header>
<body>
  <h1>Hola Mundo</h1>
  <div class="container p-4">
    <div class="row">
      <% crearUsuario.forEach(function(crarUsuario){ %>
        <div class="col-md-4">
          <div class="card">
            <div class="card-header">
                <%= crearUsuario.nombre %> / <%= crearUsuario.apellido %>
            </div>
            <div class="card-body">
              <p>
                <%= crarUsuario.email %>
              </p>
            </div>
            <div class="card-footer">
              <button class="btn btn-danger">Eliminar</button>
            </div>
          </div>
        </div>
        <% })%>

    </div>
  </div>
</body>

</html>